<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-DiaFlux-Dialog" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWE-Plugin-DiaFlux" />
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWE-Plugin-DiaFlux" point-id="DiaFluxDisplayEnhancement"
		id="DiaFluxDialog">
		<parameter id="class"
			value="de.d3web.we.diaFlux.dialog.DiaFluxDialog" />
		<parameter id="name" value="DiaFluxDialog" />
		<parameter id="description" value="Dialogfor DiaFlux" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Action"
		id="DiaFluxDialogAction">
		<parameter id="class"
			value="de.d3web.we.diaFlux.dialog.DiaFluxDialogAction" />
		<parameter id="name" value="DiaFluxDialogAction" />
		<parameter id="description" value="Action DiaFluxDialogAction" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Action"
		id="DiaFluxDialogManageAction">
		<parameter id="class"
			value="de.d3web.we.diaFlux.dialog.DiaFluxDialogManageAction" />
		<parameter id="name" value="DiaFluxDialogManageAction" />
		<parameter id="description" value="Action DiaFluxDialogManageAction" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="de.d3web.we.diaFlux.dialog.DiaFluxDialogType">
		<parameter id="class"
			value="de.d3web.we.diaFlux.dialog.DiaFluxDialogType" />
		<parameter id="name" value="de.d3web.we.diaFlux.dialog.DiaFluxDialogType" />
		<parameter id="description" value="Type de.d3web.we.diaFlux.dialog.DiaFluxDialogType" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="root" />
	</extension>
</plugin>
