<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.uniwue.d3web</groupId>
	<artifactId>KnowWE-Plugin-UIMA-Connector</artifactId>
	<packaging>jar</packaging>
	<name>KnowWE-Plugin: UIMAConnector</name>
	<version>0.2-SNAPSHOT</version>
	<url>http://www.d3web.de/</url>

	<build>
		<plugins>
			<plugin>
				<groupId>de.uniwue.maven.plugins</groupId>
				<artifactId>knowwe-versionator</artifactId>
				<version>${versionatorVersion}</version>
				<executions>
					<execution>
						<id>versionate-plugin</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>versionate-plugin</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
  <repositories>
		<repository>
    		<id>isci-uima</id>
    		<name>isci-uima</name>
    		<url>https://isci.informatik.uni-wuerzburg.de/artifactory/uima</url>
		</repository>
	</repositories>

	<dependencies>
		<dependency>
			<groupId>de.uniwue.d3web</groupId>
			<artifactId>KnowWE-core</artifactId>
			<version>0.2-SNAPSHOT</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.tomcat</groupId>
			<artifactId>servlet-api</artifactId>
			<version>6.0.20</version>
			<type>jar</type>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.uima</groupId>
			<artifactId>jVinci</artifactId>
			<version>2.2.2-incubating</version>
		</dependency>
		<dependency>
			<groupId>org.apache.uima</groupId>
			<artifactId>uimaj-core</artifactId>
			<version>2.2.2-incubating</version>
		</dependency>
		<dependency>
			<groupId>de.uniwue.dltk</groupId>
			<artifactId>textmarker_antlr</artifactId>
			<type>jar</type>
			<version>0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>de.uniwue.tm</groupId>
			<artifactId>textmarker_engine</artifactId>
			<type>jar</type>
			<version>0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.apache.uima</groupId>
			<artifactId>uimaj-document-annotation</artifactId>
			<version>2.2.2-incubating</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.1</version>
		</dependency>
		<dependency>
			<groupId>de.uniwue.d3web</groupId>
			<artifactId>KnowWE-Plugin-ExtensionPoints</artifactId>
			<version>0.2-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>de.uniwue.d3web</groupId>
			<artifactId>KnowWE-Plugin-Shared</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
</project>
