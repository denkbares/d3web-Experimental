<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="d3web-Plugin-ProKEtProperties" version="0.0.1">
	
	<requires>
		<import plugin-id="d3web-Kernel-ExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="date_format">
		<parameter id="name" value="date_format" />
		<parameter id="description" value="allows to specify a format for date entries" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="text_format">
		<parameter id="name" value="text_format" />
		<parameter id="description" value="allows to specify a format for text entries" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="popup">
		<parameter id="name" value="popup" />
		<parameter id="description" value="can be used for popups providing addtional informations for questions" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>

	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="grid">
		<parameter id="name" value="grid" />
		<parameter id="description" value="can be used for grids providing a summary of the entered values" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="image">
		<parameter id="name" value="image" />
		<parameter id="description" value="can be used for images in image questions" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="imagewidth">
		<parameter id="name" value="imagewidth" />
		<parameter id="description" value="can be used to set a specific width for the image of a question" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
		<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="imageheight">
		<parameter id="name" value="imageheight" />
		<parameter id="description" value="can be used to set a specific height for the image of a question" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
		<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="Property"
		id="imagemap">
		<parameter id="name" value="imagemap" />
		<parameter id="description" value="can be used to specify an imagemap for a image of question" />
		<parameter id="multilingual" value="false" />
		<parameter id="autosave" value="basic" />
		<parameter id="priority" value="5" />
		<parameter id="instanceof" value="java.lang.String" />
	</extension>
	
	
</plugin>