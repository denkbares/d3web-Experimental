<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>d3web-ProKEt</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>de.d3web.proket.run.HelloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dialog</servlet-name>
        <servlet-class>de.d3web.proket.run.DialogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>D3webDialog</servlet-name>
        <servlet-class>de.d3web.proket.d3web.run.D3webDialog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MediastinitisDialog</servlet-name>
        <servlet-class>de.d3web.proket.d3web.run.MediastinitisDialog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Debug</servlet-name>
        <servlet-class>de.d3web.proket.run.DebugServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlCenter</servlet-name>
        <servlet-class>de.d3web.proket.run.ControlCenter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>de.d3web.proket.run.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EuraHSDialog</servlet-name>
        <servlet-class>de.d3web.proket.d3web.run.EuraHSDialog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClarihieDialog</servlet-name>
        <servlet-class>de.d3web.proket.d3web.run.ClarihieDialog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>de.d3web.proket.d3web.run.DownloadTxtFileServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/HelloServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dialog</servlet-name>
        <url-pattern>/Dialog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>D3webDialog</servlet-name>
        <url-pattern>/D3webDialog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MediastinitisDialog</servlet-name>
        <url-pattern>/Mediastinitis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Debug</servlet-name>
        <url-pattern>/Debug</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlCenter</servlet-name>
        <url-pattern>/ControlCenter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EuraHSDialog</servlet-name>
        <url-pattern>/EuraHS-Dialog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClarihieDialog</servlet-name>
        <url-pattern>/ClarihieDialog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/Download</url-pattern>
    </servlet-mapping>
</web-app>
