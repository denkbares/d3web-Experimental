<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-CIConnector" version="0.1">

	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>

    <runtime>
        <library id="de.knowwe" path="." type="code">
            <export prefix="*" />
        </library>
        <library id="webapp" path="webapp/" type="resources">
            <export prefix="*" />
        </library>
    </runtime>
    
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="CIInfoType">
		<parameter id="class" value="com.denkbares.ciconnector.CIInfoType" />
		<parameter id="name" value="CIInfoType" />
		<parameter id="description" value="Dummy Markup that triggers the CIInfoHandler" />
		<parameter id="version" value="0.2" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="root" />
	</extension>
	    
    <extension plugin-id="KnowWEExtensionPoints" point-id="Action"
        id="SaveCIInfoAction">
        <parameter id="class"
            value="com.denkbares.ciconnector.SaveCIInfoAction" />
        <parameter id="name" value="SaveCIInfoAction" />
        <parameter id="description" value="Action SaveCIInfoAction" />
        <parameter id="version" value="1.0" />
        <parameter id="priority" value="5" />
    </extension>
    
</plugin>