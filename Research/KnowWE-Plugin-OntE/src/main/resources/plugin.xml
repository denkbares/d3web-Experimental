<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-OntE" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>

	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="TurtleMarkup">
		<parameter id="class"
			value="de.knowwe.kdom.turtle.TurtleMarkup" />
		<parameter id="name" value="TurtleMarkup" />
		<parameter id="description" value="KnowWEObjectType TurtleMarkup" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="4" />
		<parameter id="scope" value="root" />
	</extension>
	
		<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="TurtleMarkupSimple">
		<parameter id="class"
			value="de.knowwe.kdom.turtle.simple.TurtleMarkupSimple" />
		<parameter id="name" value="TurtleMarkupSimple" />
		<parameter id="description" value="KnowWEObjectType TurtleMarkupSimple" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="4" />
		<parameter id="scope" value="root" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="TagHandler"
		id="TerminologyViewHandler">
		<parameter id="class" value="de.knowwe.util.TerminologyViewHandler" />
		<parameter id="name" value="TerminologyViewHandler" />
		<parameter id="description" value="TagHandler TerminologyViewHandler" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>

	
	
</plugin>