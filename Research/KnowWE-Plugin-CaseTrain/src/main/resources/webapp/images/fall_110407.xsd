<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">
	<!-- -->
	<xs:element name="Case">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Metadata">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" type="xs:ID">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_ID"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="Title">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_TITEL"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Author" type="xs:string">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_AUTOR"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Version" type="xs:string">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_VERSION"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Comment" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_KOMMENTAR"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Todos" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Todo" type="xs:string" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation><![CDATA["FALL_TODO"]]></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="History" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_HISTORIE"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Date" type="xs:string">
								<xs:annotation>
									<xs:documentation><![CDATA["FALL_DATUM"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Score">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="weighedObject">
											<xs:annotation>
												<xs:documentation><![CDATA[weight = "FALL_PUNKTZAHL"]]></xs:documentation>
											</xs:annotation>
											<xs:sequence>
												<xs:element name="RatingScheme" minOccurs="0">
													<xs:annotation>
														<xs:documentation><![CDATA[Notenschema]]></xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="RatingElement" maxOccurs="unbounded">
																<xs:complexType>
																	<xs:attribute name="rating" type="positiveDecimal"
																		use="required" />
																	<xs:attribute name="lowerBoundary" type="positiveDecimal"
																		use="required" />
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="minScoreForSuccess" type="zeroToOne"
												use="required">
												<xs:annotation>
													<xs:documentation><![CDATA["BESTEHEN_AB"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute ref="okTimeSeconds">
												<xs:annotation>
													<xs:documentation><![CDATA["ZEIT_GRENZE100"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute ref="maxTimeSeconds">
												<xs:annotation>
													<xs:documentation><![CDATA["ZEIT_GRENZE0"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="weightTime" type="positiveDecimal">
												<xs:annotation>
													<xs:documentation><![CDATA["ZEIT_GEWICHT"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="instance_userTimeSeconds"
												type="xs:positiveInteger">
												<xs:annotation>
													<xs:documentation><![CDATA[[for resume only] total user time with this case]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="Misc" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="feedback">
										<xs:annotation>
											<xs:documentation><![CDATA["FEEDBACK"]]></xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="none">
													<xs:annotation>
														<xs:documentation><![CDATA["aus"]]></xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="endonly">
													<xs:annotation>
														<xs:documentation><![CDATA["Ende"]]></xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="available">
													<xs:annotation>
														<xs:documentation><![CDATA["verfügbar"]]></xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="auto">
													<xs:annotation>
														<xs:documentation><![CDATA["auto"]]></xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="forceauto">
													<xs:annotation>
														<xs:documentation><![CDATA["forceauto"]]></xs:documentation>
													</xs:annotation>
												</xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="colorScheme">
										<xs:annotation>
											<xs:documentation><![CDATA[[unused]]]></xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="default" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="language">
										<xs:annotation>
											<xs:documentation><![CDATA["SPRACHE" (http://en.wikipedia.org/wiki/List_of_ISO_639-1_codes)]]></xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="de" />
												<xs:enumeration value="en" />
												<xs:enumeration value="fr" />
												<xs:enumeration value="es" />
												<xs:enumeration value="it" />
												<xs:enumeration value="pt" />
												<xs:enumeration value="ru" />
												<xs:enumeration value="zh" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="showClock" type="xs:boolean">
										<xs:annotation>
											<xs:documentation><![CDATA["ZEIT_ANZEIGEN": only if okTimeSeconds & maxTimeSeconds then this setting is considered]]></xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element ref="URL" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["HINTERGRUNDWISSEN"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="URLText" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["HW_LINKTEXT"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DurationMinutes" type="xs:unsignedInt"
								minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["DAUER_MIN"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Prereqs" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["VORAUSSETZUNGEN"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Difficulty" type="xs:string"
								minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["SCHWIERIGKEIT"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Notice" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["HINWEIS"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Keywords" type="xs:string"
								minOccurs="0">
								<xs:annotation>
									<xs:documentation><![CDATA["SCHLAGWORTE"]]></xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Intro" type="titledmmcontent">
					<xs:annotation>
						<xs:documentation><![CDATA["Einleitung"]]></xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Sections">
					<xs:complexType>
						<xs:choice maxOccurs="unbounded">
							<xs:element ref="SimpleSection" />
							<xs:element name="InfoSection">
								<xs:annotation>
									<xs:documentation><![CDATA["Infowahl"]]></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="weighedObject">
											<xs:sequence>
												<xs:element name="SelectItem" type="titledmmcontent">
													<xs:annotation>
														<xs:documentation><![CDATA[placeholder for "Infowahl" data]]></xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:choice minOccurs="0" maxOccurs="unbounded">
													<xs:element name="InfoItem" minOccurs="0">
														<xs:annotation>
															<xs:documentation><![CDATA["Info" inside "Infowahl"]]></xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="basicSection">
																	<xs:attribute ref="id" use="required">
																		<xs:annotation>
																			<xs:documentation><![CDATA[*id* of corresponding *Term* in >terminology.xml]]></xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																	<xs:attribute name="value" type="xs:decimal">
																		<xs:annotation>
																			<xs:documentation><![CDATA[correctness or cost value (overwrites *Term*'s value)]]></xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="instance_UserItem" minOccurs="0"
														maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation><![CDATA[[for resume only] user chosen *Term* not refered in this xml]]></xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute ref="id" use="required" />
														</xs:complexType>
													</xs:element>
												</xs:choice>
												<xs:element name="DummyItem" minOccurs="0">
													<xs:annotation>
														<xs:documentation><![CDATA[text and value for the default ("*")]]></xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:complexContent>
															<xs:extension base="mmcontent">
																<xs:attribute name="value" type="xs:decimal" />
															</xs:extension>
														</xs:complexContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="Feedback" type="mmcontent"
													minOccurs="0">
													<xs:annotation>
														<xs:documentation><![CDATA["Infowahl\Erklärung"]]></xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element ref="Control" minOccurs="0" />
												<xs:element ref="instance_userComment" minOccurs="0" />
											</xs:sequence>
											<xs:attribute ref="id" use="required">
												<xs:annotation>
													<xs:documentation><![CDATA[*id* of corresponding root *Term* in >terminology.xml]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="type">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="cost">
															<xs:annotation>
																<xs:documentation><![CDATA["Infowahl" options "-P" (can be omitted, default)]]></xs:documentation>
															</xs:annotation>
														</xs:enumeration>
														<xs:enumeration value="points">
															<xs:annotation>
																<xs:documentation><![CDATA["Infowahl" options "-K"]]></xs:documentation>
															</xs:annotation>
														</xs:enumeration>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="lower" type="xs:decimal">
												<xs:annotation>
													<xs:documentation><![CDATA["untere Grenze"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="upper" type="xs:decimal">
												<xs:annotation>
													<xs:documentation><![CDATA["obere Grenze"]]></xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute ref="instance_userActivated" />
											<xs:attribute ref="instance_userScore" />
											<xs:attribute ref="instance_systemScore" />
											<xs:attribute ref="name" />
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="Extro">
					<xs:annotation>
						<xs:documentation><![CDATA["Abschluss"]]></xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="titledmmcontent">
								<xs:attribute ref="instance_userActivated" />
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Evaluation" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EvaluationSections">
								<xs:complexType>
									<xs:sequence maxOccurs="unbounded">
										<xs:element ref="SimpleSection">
											<xs:annotation>
												<xs:documentation><![CDATA["Evaluation" sections after *Extro*]]></xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="EvaluationEnd">
								<xs:annotation>
									<xs:documentation><![CDATA["Ende"]]></xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="titledmmcontent">
											<xs:attribute ref="instance_userActivated" />
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute ref="instance_userActivated" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- -->
	<xs:element name="Title" type="xs:string" />
	<xs:element name="URL" type="xs:anyURI" />
	<xs:element name="Text" type="xs:string" />
	<!-- -->
	<xs:element name="Content" type="xs:string" />
	<!-- -->
	<xs:complexType name="mmitem">
		<xs:sequence>
			<xs:element ref="URL" />
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="video">
						<xs:annotation>
							<xs:documentation><![CDATA["Video"]]></xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="audio">
						<xs:annotation>
							<xs:documentation><![CDATA["Audio"]]></xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="image">
						<xs:annotation>
							<xs:documentation><![CDATA["Bild" (or inline image)]]></xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!-- -->
	<xs:element name="MultimediaItem" type="mmitem" />
	<!-- -->
	<xs:complexType name="mmcontent">
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="Content" minOccurs="0" />
			<xs:element ref="MultimediaItem" minOccurs="0" />
			<xs:element ref="Formula" minOccurs="0" />
		</xs:choice>
	</xs:complexType>
	<!-- mixed wird gebraucht um abwärtskompatibel sein zu können -->
	<xs:complexType name="mmmixedcontent" mixed="true">
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="Content" minOccurs="0" />
			<xs:element ref="MultimediaItem" minOccurs="0" />
			<xs:element ref="Formula" minOccurs="0" />
		</xs:choice>
	</xs:complexType>
	<!-- -->
	<xs:element name="Formula">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TeX" type="xs:string" />
				<xs:element name="MathML" type="xs:string" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- -->
	<xs:complexType name="titledmmcontent">
		<xs:complexContent>
			<xs:extension base="mmcontent">
				<xs:sequence>
					<xs:element ref="Title">
						<xs:annotation>
							<xs:documentation><![CDATA[section title]]></xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- -->
	<xs:complexType name="basicSection">
		<xs:complexContent>
			<xs:extension base="mmcontent">
				<xs:sequence>
					<xs:element name="Questions" minOccurs="0">
						<xs:complexType>
							<xs:choice maxOccurs="unbounded">
								<xs:element name="OCQuestion" type="choiceQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "OC"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="MCQuestion" type="choiceQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "MC"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="HLMMCQuestion" type="terminologyQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "MC" Darstellung "HLM"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="HLMOCQuestion" type="terminologyQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "OC" Darstellung "HLM"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="LMMCQuestion" type="terminologyQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "MC" Darstellung "LM"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="LMOCQuestion" type="terminologyQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "OC" Darstellung "LM"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="WordQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "W"]]></xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="basicQuestion">
												<xs:sequence>
													<xs:element ref="WordAnswers" />
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="OMWordQuestion" type="multiWordQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "OMW"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="UMWordQuestion" type="multiWordQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "UMW"]]></xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="NumQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "N"]]></xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="basicQuestion">
												<xs:sequence>
													<xs:element ref="NumAnswers" />
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="MNumQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "MN"]]></xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="basicQuestion">
												<xs:sequence>
													<xs:element ref="NumAnswers" minOccurs="2"
														maxOccurs="unbounded" />
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="TextQuestion">
									<xs:annotation>
										<xs:documentation><![CDATA[Typ "T"]]></xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="basicQuestion">
												<xs:sequence>
													<xs:element name="Solution" type="xs:string">
														<xs:annotation>
															<xs:documentation><![CDATA["Antwort" content]]></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="instance_UserAnswer" type="xs:string"
														minOccurs="0">
														<xs:annotation>
															<xs:documentation><![CDATA[[for resume only] user's answer]]></xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="TextQuestionCorrectionFeatures"
														minOccurs="0">
														<xs:annotation>
															<xs:documentation><![CDATA[Korrektur-Features]]></xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:choice maxOccurs="unbounded">
																<xs:element name="KeyConceptsCorrectionFeature">
																	<xs:annotation>
																		<xs:documentation><![CDATA[Korrektur-Feature für Schlüsselkonzepte]]></xs:documentation>
																		<xs:documentation><![CDATA[hat immer: evaluationTargetSet="percentageAny"]]></xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:complexContent>
																			<xs:extension base="correctionFeature">
																				<xs:sequence>
																					<xs:element name="KeyConcepts">
																						<xs:complexType>
																							<xs:choice maxOccurs="unbounded">
																								<xs:element name="KeyConcept">
																									<xs:complexType>
																										<xs:complexContent>
																											<xs:extension base="weighedObject">
																												<xs:sequence>
																													<xs:element ref="Text" />
																												</xs:sequence>
																												<xs:attribute ref="evaluationTargetSet" />
																											</xs:extension>
