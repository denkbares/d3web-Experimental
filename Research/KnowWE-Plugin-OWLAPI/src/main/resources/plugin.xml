<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-OWLAPI" version="0.0.1">

	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>

	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="EventListener"
		id="RDF2GoEventListener">
		<parameter id="class"
			value="de.knowwe.owlapi.RDF2GoEventListener" />
		<parameter id="name" value="RDF2GoEventListener" />
		<parameter id="description" value="EventListener RDF2GoEventListener" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
    <extension plugin-id="KnowWEExtensionPoints" point-id="Type"
        id="PelletSparqlSelect">
        <parameter id="class" value="de.knowwe.owlapi.query.pellet.PelletSparqlSelect" />
        <parameter id="name" value="PelletSparqlSelect" />
        <parameter id="description"
            value="Select Markup for the Pellet Sparql Query Engine" />
        <parameter id="version" value="1.0" />
        <parameter id="priority" value="6" />
        <parameter id="scope" value="root" />
    </extension>	
    <extension plugin-id="KnowWEExtensionPoints" point-id="Type"
        id="PelletSparqlAsk">
        <parameter id="class" value="de.knowwe.owlapi.query.pellet.PelletSparqlAsk" />
        <parameter id="name" value="PelletSparqlAsk" />
        <parameter id="description"
            value="Ask Markup for the Pellet Sparql Query Engine" />
        <parameter id="version" value="1.0" />
        <parameter id="priority" value="6" />
        <parameter id="scope" value="root" />
    </extension>    	

</plugin>