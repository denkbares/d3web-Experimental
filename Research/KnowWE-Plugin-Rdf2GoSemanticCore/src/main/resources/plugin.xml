		<parameter id="name" value="DefaultMarkupOwlHandlerForRdf2Go" />
		<parameter id="description" value="Subtreehandler for Defaultmarkup to add owl knowledge" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
