<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-Rdf2GoSemanticCore" version="0.0.1">
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="AddStatementType">
		<parameter id="class"
			value="de.d3web.we.core.semantic.rdf2go.example.AddStatementType" />
		<parameter id="name" value="AddStatementType" />
		<parameter id="description" value="KnowWEObjectType AddStatementType" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="3" />
		<parameter id="scope" value="root" />
	</extension>
	
		<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="SparqlType">
		<parameter id="class"
			value="de.d3web.we.core.semantic.rdf2go.example.SparqlType" />
		<parameter id="name" value="SparqlType" />
		<parameter id="description" value="KnowWEObjectType SparqlType" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="3" />
		<parameter id="scope" value="root" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="TagHandler"
		id="DumpHandler">
		<parameter id="class"
			value="de.d3web.we.core.semantic.rdf2go.example.DumpHandler" />
		<parameter id="name" value="DumpHandler" />
		<parameter id="description" value="TagHandler DumpHandler" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
</plugin>
