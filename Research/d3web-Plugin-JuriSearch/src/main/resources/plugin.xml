<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="d3web-JuriSearch" version="0.0.1">

	<requires>
		<import plugin-id="d3web-Kernel-ExtensionPoints" />
		<import plugin-id="KnowledgePersistenceExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="d3web-Kernel-ExtensionPoints" point-id="PSMethod"
		id="PSMethodJuri">
		<parameter id="class"
			value="de.d3web.jurisearch.PSMethodJuri" />
		<parameter id="name" value="PSMethodJuri" />
		<parameter id="description" value="PSMethod for Juri" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowledgePersistenceExtensionPoints" point-id="KnowledgeReader"
		id="JuriRulePersistenceHandler">
		<parameter id="class"
			value="de.d3web.jurisearch.JuriRulePersistenceHandler" />
		<parameter id="name" value="JuriRulePersistenceHandler" />
		<parameter id="description" value="KnowledgeReader for JuriRules" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="filename" value="kb/juri.xml" />
	</extension>
	
	<extension plugin-id="KnowledgePersistenceExtensionPoints" point-id="KnowledgeWriter"
		id="JuriRulePersistenceHandler.Writer">
		<parameter id="class"
			value="de.d3web.jurisearch.JuriRulePersistenceHandler" />
		<parameter id="name" value="JuriRulePersistenceHandler" />
		<parameter id="description" value="KnowledgeWriter for JuriRules" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="filename" value="kb/juri.xml" />
	</extension>
</plugin>