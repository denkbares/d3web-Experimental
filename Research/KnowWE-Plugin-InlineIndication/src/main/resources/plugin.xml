<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-InlineIndication" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="QuestionTreeRootTypeForQuestionLine">
		<parameter id="class"
			value="de.d3web.we.kdom.questionTree.extension.InlineIndicationCondition" />
		<parameter id="name" value="InlineIndicationCondition" />
		<parameter id="description" value="Type InlineIndicationCondition to be added to QuestionLine" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="QuestionLine" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="QuestionTreeRootTypeForQClassLine">
		<parameter id="class"
			value="de.d3web.we.kdom.questionTree.extension.InlineIndicationCondition" />
		<parameter id="name" value="InlineIndicationCondition" />
		<parameter id="description" value="Type InlineIndicationCondition to be added to QClassLine" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="QClassLine" />
	</extension>

</plugin>