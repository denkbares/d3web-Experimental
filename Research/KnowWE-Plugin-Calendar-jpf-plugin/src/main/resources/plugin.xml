<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-Calendar" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Instantiation"
		id="CalendarModule">
		<parameter id="class"
			value="de.d3web.we.plugin.calendar.CalendarModule" />
		<parameter id="name" value="CalendarModule" />
		<parameter id="description" value="Instantiation CalendarModule" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Action"
		id="CalendarAction">
		<parameter id="class"
			value="de.d3web.we.plugin.calendar.CalendarAction" />
		<parameter id="name" value="CalendarAction" />
		<parameter id="description" value="Action CalendarAction" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="Appointment">
		<parameter id="class"
			value="de.d3web.we.plugin.calendar.Appointment" />
		<parameter id="name" value="Appointment" />
		<parameter id="description" value="KnowWEObjectType Appointment" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="root" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="TagHandler"
		id="CalendarHandler">
		<parameter id="class"
			value="de.d3web.we.plugin.calendar.CalendarHandler" />
		<parameter id="name" value="CalendarHandler" />
		<parameter id="description" value="TagHandler CalendarHandler" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>

</plugin>